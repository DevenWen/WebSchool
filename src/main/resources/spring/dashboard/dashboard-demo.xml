    <!-- 控制台demo -->
    <db:DashboardEntry id="demodashboard" authority="10" title="demoDashboard"
    	icon="glyphicon glyphicon-duplicate">
    
    	<!-- Spring翻译 --> 
    	<db:topkids>
    		<db:TopNarPoint title="top导航1" url="wwww.i.am.url.com"
    			icon="glyphicon glyphicon-duplicate"></db:TopNarPoint>
    		<db:TopNarPoint title="top导航2" url="www.i.am.url.com"
    			icon="glyphicon glyphicon-duplicate"></db:TopNarPoint>
    	</db:topkids>
    	<db:sidekids> 
    		<db:Barpoint title="公告" type="leave" 
    			icon="glyphicon glyphicon-list-alt" url=":{path}/index.jsp"></db:Barpoint>
    		<db:Barpoint title="公共下载" type="leave" 
    			icon="glyphicon glyphicon-arrow-down" url=":{path}/index.jsp"></db:Barpoint>
    		<db:Barpoint title="综合测评" type="parent" 
    			icon="glyphicon glyphicon-duplicate active" url="##">
    			<db:kids>
    			<db:Barpoint title="自评" type="leave" 
    				icon="glyphicon glyphicon-pencil" url="##"></db:Barpoint>
    			<db:Barpoint title="班评" type="leave" 
    				icon="glyphicon glyphicon-pencil" url="##"></db:Barpoint>
    			<db:Barpoint title="系评" type="leave" 
    				icon="glyphicon glyphicon-pencil" url="##"></db:Barpoint>
    			</db:kids>
    		</db:Barpoint>
    		<db:Barpoint title="个人信息" type="leave" 
    			icon="glyphicon glyphicon-user" url="##"></db:Barpoint>
    		<db:Barpoint title="其他" type="parent" 
    			icon="glyphicon glyphicon-plus" url="##">
    			<db:kids>
    				<db:Barpoint title="授权" type="leave" 
    					icon="glyphicon glyphicon-thumbs-up" url="##"/>
    				<db:Barpoint title="修改个人密码" type="leave" 
    					icon="glyphicon glyphicon-lock" url="##"/>
    				<db:Barpoint icon="glyphicon glyphicon-duplicate" 
    				 title="二级导航1" type="parent" url="##">
    				 	<db:kids>
		    				<db:Barpoint icon="glyphicon glyphicon-duplicate"
		    				 title="三级导航1" type="leave" url=":{path}/view/login.jsp"/>
		    				<db:Barpoint icon="glyphicon glyphicon-duplicate" 
		    				 title="三级导航2" type="leave" url="##"/>
		    			</db:kids>
    				 </db:Barpoint>    				
    			</db:kids>
    		</db:Barpoint>
    	</db:sidekids> 
    	   
    </db:DashboardEntry>
    
